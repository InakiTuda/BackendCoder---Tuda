<form id="formProduct">
    <h2>Real Time Products</h2>
    <input class="controls" type="text" name="title" id="title" placeholder="Ingrese el nombre del producto" required>
    <input class="controls" type="text" name="description" id="description" placeholder="Ingrese la descripción del producto" required>
    <input class="controls" type="number" name="stock" id="stock" placeholder="Ingrese la cantidad del producto" required>
    <input class="controls" type="text" name="thumbnail" id="thumbnail" placeholder="Inserte imagen">
    <input class="controls" type="text" name="category" id="category" placeholder="Ingrese la categoria del producto" required>
    <input class="controls" type="number" name="price" id="price" placeholder="Ingrese el precio del producto" required>
    <input class="controls" type="text" name="code" id="code" placeholder="Ingrese el código del producto" required>
    <button class="buttons" type="submit" id="add-btn">Ingresar Producto</button>
    <h2>Eliminar un Producto: </h2>
    <input class="controls" type="number" name="id-prod" id="id-prod" placeholder="Ingrese ID del producto">
    <button class="buttons" type="button" id="delete-btn">Eliminar Producto</button>


</form>

<article id="list-products">

</article>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/realtime.js"></script>